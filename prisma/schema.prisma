generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String @id @default(uuid())
  name         String
  lastname     String
  email        String @unique
  passwordHash String
  dob          String

  phone         String?
  document      String?
  document_type String?

  role String @default("USER")

  @@map("users")
}
